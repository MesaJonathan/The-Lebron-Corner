{
  "name": "Lebron Corner",
  "description": "Toggle the switch in the pannel to actiavte Lebron James highlights in the corner.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "pannel/pannel.html",
    "default_icon": "lebron_icon.png"

  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
  